{
  "name": "react-ssr",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.9.0"
  },
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.3",
    "react-router-dom": "^6.4.2",
    "reactstrap": "^9.1.5",
    "request": "^2.79.0",
    "resolve": "1.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "babel-loader": "8.1.0",
    "babel-preset-react-app": "10.0.0",
    "cross-env": "^7.0.3",
    "nodemon-webpack-plugin": "^4.8.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "webpack": "^5.74.0",
    "webpack-assets-manifest": "^5.1.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "pretty:fix": "prettier --write .",
    "pretty": "prettier --check .",
    "prebuild": "rimraf ./build",
    "build": "cross-env NODE_ENV=production npm-run-all -s build:*",
    "build:server": "webpack --config webpack.server.config.js",
    "build:client": "webpack --config webpack.client.config.js",
    "start": "node build/bundle.js",
    "dev:server": "webpack --config webpack.server.config.js --watch",
    "dev:client": "webpack serve --config webpack.client.config.js --profile --json=compilation-stats.json",
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel dev:*"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "runtime": "automatic"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  }
}
